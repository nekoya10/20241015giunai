   "source": [
    "try:\n",
    "    x = int(input(\"請輸入整數\"))\n",
    "    print(f\"{x}的公因數是:\",end='')\n",
    "    for j in range(1,x+1):\n",
    "        if(x%j==0):\n",
    "            print(j,end=' ')\n",
    "except Exception:    \n",
    "    print(\"格式錯誤\") \n"
   ]
